{
  "name": "active-session-library",
  "version": "1.0.x",
  "private": true,
  "author": "crazy-slot",
  "license": "MIT",
  "repository": "crazy-slot/active-session-library",
  "workspaces": [
    "examples/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@changesets/cli": "^2.24.4",
    "commitizen": "^4.2.5",
    "eslint-config-custom": "*",
    "git-cz": "^4.9.0",
    "prettier": "^2.7.1",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "resolutions": {
    "nth-check": ">=2.0.1"
  },
  "packageManager": "yarn@1.22.19",
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
